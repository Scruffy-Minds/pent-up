    <div id="socials" class="row space-around icons">
        <%const socials = linkData.sites.filter(x => x.type === 'social'); socials.forEach((v, i, a) => {%>
        <%-` <a href="${v.url}" target="${v.target}" style="order: ${i};">
            <picture>
                <source srcset="/images/icons/socials/${v.id}.webp" type="image/webp">
                <img id="${v.id}" class="social-icon" src="/images/icons/socials/${v.id}.png" alt="${v.name}">
            </picture>
        </a>
        `%><%})%>
    </div>
    <div class="row copyright center footer-tile"><% const thisYear = new Date().getFullYear(); %>
        <span>&copy;<%- thisYear %>, Pent Up! All rights reserved.</span>
    </div>
    </div>

</body>

</html>